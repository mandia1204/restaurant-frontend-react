{
    "files.exclude": {
        "node_modules/": true
    },
    "eslint.enable": true,
    "eslint.lintTask.enable": true,
    "editor.tabSize": 2,
    "eslint.validate":[
        "javascript",
        "javascriptreact",
        "typescript",
        "typescriptreact"
    ],"eslint.options": {
        "extensions": [
            ".jsx",
            ".js",
            ".ts",
            ".tsx"
        ]
	},
    "files.associations": {
        "Dockerfile*": "dockerfile"
    },
    "typescript.tsdk": "node_modules\\typescript\\lib",
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": true
    }
}