{
  "env": {
    "production": {
      "presets": [
        [ "@babel/env", { "modules": false } ], 
        "@babel/preset-react"
      ],
      "plugins": [
        "@babel/proposal-class-properties",
        "@babel/proposal-object-rest-spread",
      ]
    },
    "test": {
      "presets": ["@babel/env", "@babel/preset-react"],
      "plugins": [
        "@babel/proposal-class-properties",
        "@babel/proposal-object-rest-spread",
      ]
    },
    "development": {
      "presets": ["@babel/env", "@babel/preset-react"],
      "plugins": [
        "@babel/proposal-class-properties",
        "@babel/proposal-object-rest-spread",
      ]
    }
  }
}