    "build-dev": "cross-env NODE_ENV=development webpack --config ./webpack/dev.js",
    "start-prod": "lws --port=9000 --dir=dist/",
    "build-backup": "cross-env BABEL_ENV=production webpack --config ./webpack.config.js",
    "node-test": "cross-env BABEL_ENV=test babel-node --extensions '.js,.ts,.jsx,.tsx' runNodeTests.js src/**/*.test.js | faucet",
    "check-types": "tsc",
    "test": "cross-env BABEL_ENV=test NODE_ENV=test karma start",
    "debug": "babel-node --nolazy --inspect-brk=9229 src/components/login/LoginForm.test.js",
